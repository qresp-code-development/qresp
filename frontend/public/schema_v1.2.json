      "$id": "#/properties/schema",
      "type": ["string"],
      "title": "The Schema Schema",
      "default": "",
      "examples": ["http://paperstack.uchicago.edu/v1_2.json"],
      "pattern": "^(.*)$"
    },
    "scripts": {
      "$id": "#/properties/scripts",

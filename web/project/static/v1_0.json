              "examples": [
                "H."
              ],
              "pattern": "^(.*)$"
            }
          },
          "required": [
            "firstName",
            "lastName"
          ]
        },
        "notebookFile": {
          "$id": "#/properties/info/properties/notebookFile",
          "type": ["string","null"],
          "title": "The Notebookfile Schema",
          "default": "",
          "examples": [
            "Main.ipynb"
          ],
          "pattern": "^(.*)$"
        },
        "notebookPath": {
          "$id": "#/properties/info/properties/notebookPath",
          "type": ["string","null"],
          "title": "The Notebookpath Schema",
          "default": "",
